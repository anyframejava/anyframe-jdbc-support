<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="jdbc-support" description="log SQL and replace SQL with the given pattern for preventing SQL Injection">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-jdbc-support-pi</artifactId>
  <version>1.0.2</version>
  <dependent-plugins>
    <dependent-plugin name="core" version="2.0.0&gt;*"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*.xml"/>
      <exclude name="log4j.xml"/>
    </resource>
  </resources>
  <build>
    <filesets>
      <fileset dir="src/main/resources" filtered="true" packaged="false">
        <include name="**/context-jdbc-support.xml"/>
        <include name="**/log4j.xml"/>
      </fileset>
    </filesets>
  </build>
  <samples>false</samples>
  <interceptor>
    <class></class>
  </interceptor>
  <essential>false</essential>
</plugin>