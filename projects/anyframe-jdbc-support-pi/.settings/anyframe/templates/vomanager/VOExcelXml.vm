#macro( recursive $list $level $className)
 #foreach($e in $list)
  #if( "$!e.nameKo" == "" )
   #set($korName = "")
  #else
   #set($korName = $!e.nameKo)
  #end	## end if
  #if( "$!e.length" == "" )
   #set($length = "")
  #else
   #set($length = $e.length)
  #end	## end if
  #if( "$!e.description" == "" )
   #set($description = "")
  #else
   #set($description = $e.description)
  #end	## end if
  #if($e.getRefClass() != "") ## Inner VO/List type
   #set($indexCount = $e.getRefClass().lastIndexOf(".") +1)
    <Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">$className@$e.name</Data></Cell>
		<Cell><Data ss:Type="String">$korName</Data></Cell>
  		#if($e.type != "java.util.List")
		 <Cell><Data ss:Type="String">$e.getRefClass().substring($indexCount)</Data></Cell>
        #else
         #set($typeIndex = $e.type.lastIndexOf(".") +1)
		 <Cell><Data ss:Type="String">$e.type.substring($typeIndex)&lt;$e.getRefClass().substring($indexCount)&gt;</Data></Cell>
        #end
		<Cell><Data ss:Type="Number">$length</Data></Cell>
		<Cell><Data ss:Type="Number">$level</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">$description</Data></Cell>
	</Row>
   #set($levelCount = $level +1)
   #recursive( $e.children $levelCount $e.name)
  #else
   #if ($e.type == "int" || $e.type == "java.math.BigDecimal" || $e.type == "java.math.BigInteger")
    #set($type = "N")
   #elseif ($e.type == "sli.in.util.Money")
    #set($type = "Money")
   #else
    #set($type = "C")
   #end
   #if($e.parent.getRefClass() != "") ##
    #if($e.type == "sli.in.util.Money")
    #set($innerLevel = $level + 1)
    #set($indexCount = $e.parent.getRefClass().lastIndexOf(".") +1)
    <Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">$e.parent.getRefClass().substring($indexCount)@$e.name</Data></Cell>
		<Cell><Data ss:Type="String">$korName</Data></Cell>
		<Cell><Data ss:Type="String">$type</Data></Cell>
		<Cell><Data ss:Type="Number">0</Data></Cell>
		<Cell><Data ss:Type="Number">$level</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">$description</Data></Cell>
	</Row>  
	<Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">Money@amount</Data></Cell>
		<Cell><Data ss:Type="String">금액</Data></Cell>
		<Cell><Data ss:Type="String">N</Data></Cell>
		<Cell><Data ss:Type="Number">$length</Data></Cell>
		<Cell><Data ss:Type="Number">$innerLevel</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">금액</Data></Cell>
	</Row>  
	<Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">Money@currency</Data></Cell>
		<Cell><Data ss:Type="String">통화</Data></Cell>
		<Cell><Data ss:Type="String">C</Data></Cell>
		<Cell><Data ss:Type="Number">2</Data></Cell>
		<Cell><Data ss:Type="Number">$innerLevel</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">통화(원/달러 등 구분)</Data></Cell>
	</Row>  
    #else
    #set($indexCount = $e.parent.getRefClass().lastIndexOf(".") +1)
    <Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">$e.parent.getRefClass().substring($indexCount)@$e.name</Data></Cell>
		<Cell><Data ss:Type="String">$korName</Data></Cell>
		<Cell><Data ss:Type="String">$type</Data></Cell>
		<Cell><Data ss:Type="Number">$length</Data></Cell>
		<Cell><Data ss:Type="Number">$level</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">$description</Data></Cell>
	</Row>
	#end
   #elseif($e.type == "sli.in.util.Money")
    #set($innerLevel = $level + 1)
    <Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">$className@$e.name</Data></Cell>
		<Cell><Data ss:Type="String">$korName</Data></Cell>
		<Cell><Data ss:Type="String">$type</Data></Cell>
		<Cell><Data ss:Type="Number">0</Data></Cell>
		<Cell><Data ss:Type="Number">$level</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">$description</Data></Cell>
	</Row>  
	<Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">Money@amount</Data></Cell>
		<Cell><Data ss:Type="String">금액</Data></Cell>
		<Cell><Data ss:Type="String">N</Data></Cell>
		<Cell><Data ss:Type="Number">$length</Data></Cell>
		<Cell><Data ss:Type="Number">$innerLevel</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">금액</Data></Cell>
	</Row>  
	<Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">Money@currency</Data></Cell>
		<Cell><Data ss:Type="String">통화</Data></Cell>
		<Cell><Data ss:Type="String">C</Data></Cell>
		<Cell><Data ss:Type="Number">2</Data></Cell>
		<Cell><Data ss:Type="Number">$innerLevel</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">통화(원/달러 등 구분)</Data></Cell>
	</Row>  
   #else
    <Row ss:AutoFitHeight="0" ss:Height="27">
		<Cell><Data ss:Type="String">$className@$e.name</Data></Cell>
		<Cell><Data ss:Type="String">$korName</Data></Cell>
		<Cell><Data ss:Type="String">$type</Data></Cell>
		<Cell><Data ss:Type="Number">$length</Data></Cell>
		<Cell><Data ss:Type="Number">$level</Data></Cell>
		<Cell ss:Index="17"><Data ss:Type="String">$description</Data></Cell>
	</Row>
   #end ##end if
  #end ##end if
 #end ##end foreach
#end ##end macro

#recursive( $list 1 $className)
